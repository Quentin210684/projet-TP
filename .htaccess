RewriteEngine On

RewriteRule ^admin-ajouter-un-article$ admin/addArticleAdmin.php
RewriteRule ^admin-ajouter-un-jeu$ admin/addGameAdmin.php
RewriteRule ^admin-ajouter-un-mod$ admin/addModAdmin.php
RewriteRule ^liste-admin-articles$ admin/articlesListAdmin.php
RewriteRule ^administration$ admin/dashboardAdmin.php
RewriteRule ^admin-liste-des-jeux$ admin/gamesListAdmin.php
RewriteRule ^admin-liste-des-mods$ admin/modsListAdmin.php
