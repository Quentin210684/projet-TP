RewriteRule inscription registration.php
RewriteRule mentions-Legales mentionsLegales.php
RewriteRule contact contact.php
RewriteRule premiere-visite premiereVisite.php
RewriteRule ^evaluation_([0-9]+)$ addEvaluation.php?id=$1
RewriteRule ^commentaire_([0-9]+)$ addComment.php?id=$1
RewriteRule ^user-modification-commentaire_([0-9]+)$ addCommentModif.php?id=$1
RewriteRule ^user-modification-avis_([0-9]+)$ addEvaluationModif.php?id=$1
RewriteRule ^page-Article-Presse_([0-9]+)$ article.php?id=$1
RewriteRule ^selection_moment_([0-9]+)$ selectionmoment.php?id=$1
RewriteRule erreur_404 page404.php

  

RewriteRule utilisateur-ajouter-un-mod addMod.php
RewriteRule mes-avis addOpinion.php
RewriteRule mes-commentaires addCommentUsers.php